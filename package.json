{
    "name": "vscode-starless-monokai",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "license": "MIT",
    "scripts": {
        "build": "npm run clean && npm run compile && npm run generate",
        "compile": "tsc",
        "compile:watch": "tsc --watch",
        "generate": "node ./dist/script/index.js",
        "generate:watch": "nodemon ./dist/script/index.js",
        "clean": "rm -rf ./dist",
        "lint": "eslint src --ext ts"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^1.0.0-beta.28",
        "@types/node": "^20.8.7",
        "@types/vscode": "^1.78",
        "eslint": "^8.52.0",
        "nodemon": "^3.0.1",
        "typescript": "^5.2.2"
    },
    "engines": {
        "node": ">18.0.0"
    }
}
